# .env

# Required by BigQueryClient
BQ_PROJECT_ID=my-gcp-project-id
PROJECT_DATASET=my-gcp-dataset

# Tables
WAITLIST_TABLE=waitlist
APPOINTMENTS_TABLE=appointments
DEPARTMENTS_TABLE=departments
HOSPITALS_TABLE=hospitals
REJECTED_APPOINTMENTS_TABLE=rejected_appointments
USERS_TABLE=users

# Frontend
FRONTEND_URL=http://localhost:3000

# Agent
AGENT_RESOURCE_ID=your-agent-resource-id
PREF_RANKING_AGENT_RESOURCE_ID=your-agent-resource-id
AGENT_LOCATION=europe-west1
AGENT_STAGING_BUCKET=gs://your-staging-bucket

# Optional (for dev/debug conditions)
ENV=development
